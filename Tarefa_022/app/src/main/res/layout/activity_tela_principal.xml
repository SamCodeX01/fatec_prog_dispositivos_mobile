        app:layout_constraintEnd_toEndOf="@+id/btExe05"
        app:layout_constraintStart_toStartOf="@+id/btExe05"
        app:layout_constraintTop_toBottomOf="@+id/btExe05" />

    <Button
        android:id="@+id/btExe02"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Exer_02_Java_Calculo_Hipotenusa"
        app:layout_constraintBottom_toTopOf="@+id/btExe03"
        app:layout_constraintEnd_toEndOf="@+id/btExe01"
        app:layout_constraintStart_toStartOf="@+id/btExe01"
        app:layout_constraintTop_toBottomOf="@+id/btExe01" />

</androidx.constraintlayout.widget.ConstraintLayout>